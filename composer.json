{
  "name": "nicebooks/isbn",
  "description": "ISBN tools",
  "type": "library",
  "keywords": [
    "ISBN",
    "ISBN-10",
    "ISBN-13",
    "Book",
    "Books",
    "NiceBooks"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "ext-dom": "*",
    "brick/varexporter": "~0.7.0",
    "phpunit/phpunit": "^11.5",
    "php-coveralls/php-coveralls": "^2.5.2",
    "carthage-software/mago": "1.0.3"
  },
  "autoload": {
    "psr-4": {
      "Nicebooks\\Isbn\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nicebooks\\Isbn\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "carthage-software/mago": true
    }
  }
}
